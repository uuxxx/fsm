{
  "name": "@uuxxx/fsm",
  "version": "1.2.2",
  "scripts": {
    "build": "rolldown -c rolldown.config.ts",
    "changeset:version": "changeset version && git add --all && git commit -m 'chore: bump'",
    "changeset:publish": "changeset publish",
    "test": "jest",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "check:types": "tsc --noEmit"
  },
  "author": "Artem Tryapichnikov <golysheeet@gmail.com>",
  "license": "MIT",
  "description": "Javascript library for creating finite state machine",
  "homepage": "https://github.com/uuxxx/fsm/blob/main/README.md",
  "type": "module",
  "packageManager": "pnpm@10.4.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uuxxx/fsm.git"
  },
  "keywords": [
    "finite state machine",
    "fsm"
  ],
  "engines": {
    "node": " >= 20",
    "pnpm": ">= 10",
    "npm": "Please use pnpm instead of yarn to install dependencies",
    "yarn": "Please use pnpm instead of yarn to install dependencies"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./history-plugin": {
      "types": "./dist/history-plugin.d.ts",
      "default": "./dist/history-plugin.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@changesets/cli": "^2.29.7",
    "@swc/core": "^1.13.5",
    "@types/jest": "^30.0.0",
    "babel-jest": "^30.2.0",
    "eslint": "^9.37.0",
    "eslint-config-xo-typescript": "^9.0.0",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "lefthook": "^2.0.2",
    "rolldown": "^1.0.0-beta.41",
    "rolldown-plugin-dts": "^0.22.1",
    "rollup-plugin-swc3": "^0.12.1",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
